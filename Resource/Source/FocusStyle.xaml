<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"                    
                    xmlns:local="clr-namespace:Resource.AssemblySource"                   
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type local:FocusVisualStyleASB},ResourceId=focus}">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Rectangle Margin="-2" RadiusX="4" RadiusY="4" StrokeThickness="1" Stroke="{DynamicResource {x:Static local:ColorASB.BD700}}" StrokeDashArray="8 4"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>